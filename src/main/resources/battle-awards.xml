    <bean id="botsBattleAwardSettings" class="com.pragmatix.app.settings.SimpleBattleSettings">
        <property name="minLevel" value="#{levelCreator.minLevel}"/>
        <property name="maxLevel" value="#{levelCreator.maxLevel}"/>
        <property name="awardLowLevel" ref="awardLowLevel"/>
        <property name="awardMyLevel" ref="awardMyLevel"/>
